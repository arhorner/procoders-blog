<div class="c-articles">
  <div class="c-articles__title">
    <h1><strong>All Articles</strong></h1>
  </div>
    <div class="c-articles__list">
        <table>
          <tr>
            <th colspan="3"></th>
          </tr>


        <% @articles.each do |article| %>
          <tr>
            <td><h1><%= article.title %></h1></td>
            <td><p><%= link_to 'Show', article_path(article) %></p></td>
          </tr>
        <% end %>
      </table>
    </div>
  </div>
